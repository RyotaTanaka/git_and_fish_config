[alias]
	a = add
	aa = add --all
	b = branch
	bd = branch -d
	bD = branch -D
	bm = branch -m
	br = branch -r
	bu = branch -u
	cam = commit --amend
	cm = commit -m
	co = checkout
	cob = checkout -b
	cp = cherry-pick
	d = diff
	ds = diff --stat
	fap = fetch --all --prune
	g = grep
	h = help -a
	l = log
	lg = log --grep
	lo = log --oneline
	lp = log -p
	ls = log --stat
	m = merge
	ra = rebase --autostash
	rba = rebase --abort
	rbc = rebase --continue
	rbs = rebase --skip
	ri = rebase -i
	rl = reflog
	rs = reset --soft
	rv = revert -m 1
	p = push origin
	pf = push -f origin
	sa = stash apply
	sp = stash pop
	ss = stash save
	st = status

